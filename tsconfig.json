{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"target": "ESNext",
		"jsx": "react",
		"allowImportingTsExtensions": true,
		"experimentalDecorators": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"sourceMap": true
	},
	"exclude": ["node_modules", "**/node_modules/*"]
}
